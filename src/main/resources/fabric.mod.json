{
	"schemaVersion": 1,
	"id": "snekplushies",
	"version": "${version}",
	"name": "Snek Plushies",
	"description": "Add's peoples slithery fursona as a marketable plushie!",
	"authors": [
		"Churro the turtle plushie"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/snekplushies/mod_icon.png",
	"environment": "*",
